/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */


struct ListNode* deleteDuplicates(struct ListNode* head){
    if (!head || !(head->next))
        return head;

    struct ListNode *cur, *tmp;
    cur = head;


    while (cur->next != NULL)
    {
        if(cur->val == cur->next->val)
        {
            tmp = cur->next;
            cur->next = tmp->next;
            free(tmp);
        }
        else
            cur = cur->next;
    }

    return head;
}

